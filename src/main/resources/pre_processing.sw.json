{
  "id": "pre_processing",
  "version": "1.0",
  "specVersion": "0.8",
  "name": "Pre processing",
  "description": "Pre processing",
  "start": "Start",
  "functions": [
    {
      "name": "python",
      "type": "custom",
      "operation": "script:python"
    }
  ],
  "states": [
    {
      "name": "Start",
      "type": "operation",
      "actions": [
        {
          "functionRef": {
            "refName": "python",
            "arguments": {
              "script": "import sys\nsys.path.append('./python')\nfrom my_script import today\nday = today()"
            }
          }
        }
      ],
      "stateDataFilter" : {
        "output" : "{today:$WORKFLOW.python.day}"
      },
      "transition": "Pre Processing Data"
    },
    {
      "name": "Pre Processing Data",
      "type": "inject",
      "data": {
        "preProcessingData": "Data was pre processed"
      },
      "end": true
    }
  ]
}